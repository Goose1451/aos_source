        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AddAddressDto">
        <xs:sequence>
            <xs:element name="addressLine1" type="xs:string" form="qualified"/>
            <xs:element name="addressLine2" type="xs:string" form="qualified"/>
            <xs:element name="city" type="xs:string" form="qualified"/>
            <xs:element name="country" type="xs:string" form="qualified"/>
            <xs:element name="state" type="xs:string" form="qualified"/>
            <xs:element name="postalCode" type="xs:string" form="qualified"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="GetAllAccountsRequest"/>
    <xs:element name="GetCountriesRequest">
            <xs:complexType>
                <xs:sequence/>
            </xs:complexType>
    </xs:element>

    <xs:complexType name="CountryDto">
        <xs:sequence>
            <xs:element name="id" type="xs:long" />
            <xs:element name="name" type="xs:string" />
            <xs:element name="isoName" type="xs:string" />
            <xs:element name="phonePrefix" type="xs:int" />
        </xs:sequence>
    </xs:complexType>

    <xs:element name="GetCountriesResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Country" type="tns:CountryDto"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CountrySearchResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Country" type="tns:CountryDto"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetAddressesByAccountIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="accountId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetAccountByIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="accountId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetAccountByIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="AccountResponse" type="tns:AccountDto" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetAddressesByAccountIdResponse">
    <xs:complexType>
        <xs:sequence>
            <xs:element name="ShippingAddress" type="tns:AccountDto" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    </xs:element>

    <xs:element name="AccountCreateRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="accountType" type="xs:int"/>
                <xs:element name="address" type="xs:string"/>
                <xs:element name="allowOffersPromotion" type="xs:string"/>
                <xs:element name="cityName" type="xs:string"/>
                <xs:element name="countryId" type="xs:long"/>
                <xs:element name="email" type="xs:string"/>
                <xs:element name="firstName" type="xs:string"/>
                <xs:element name="lastName" type="xs:string"/>
                <xs:element name="loginName" type="xs:string"/>
                <xs:element name="password" type="xs:string"/>
                <xs:element name="phoneNumber" type="xs:string"/>
                <xs:element name="stateProvince" type="xs:string"/>
                <xs:element name="zipcode" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="AccountCreateResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="StatusMessage" type="tns:AccountStatusResponse"/>
            </xs:sequence>
        </xs:complexType>
