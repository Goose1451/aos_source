  <profiles>
    <!--<profile xmlns="http://maven.apache.org/POM/4.0.0"><id>production</id><build><plugins>/*prod plugins vvv*//*prod plugins ^^^*/</plugins></build></profile>-->
    <profile>
      <id>development</id>
      <build>
        <plugins>
          <!--dev plugins vvv-->
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-war-plugin</artifactId>
            <version>2.2</version>
            <configuration>
              <warSourceExcludes>node_modules/**</warSourceExcludes>
              <failOnMissingWebXml>false</failOnMissingWebXml>
            </configuration>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-assembly-plugin</artifactId>
            <version>2.5.4</version>
          </plugin>
          <!--dev plugins ^^^-->
        </plugins>
      </build>
    </profile>
  </profiles>
  <build>
    <!--<finalName>ROOT</finalName>-->
    <plugins>
        <!--      <plugin>
                <groupId>com.soebes.maven.plugins</groupId>
